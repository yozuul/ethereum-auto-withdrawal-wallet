{
  "name": "ethereum-auto-withdrawal-wallet-server",
  "version": "1.0.0",
  "description": "Автоматическое генерирование кошельков, проверка баланса и вывод на указанные вручную кошельки",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "SET NODE_ENV=development&& electron .",
    "dev": "nodemon --config nodemon.json",
    "roll": "rollup --config",
    "make": "electron-packager . --platform=win32 --arch=x64 EthereumAutoWithdrawal --overwrite"
  },
  "keywords": ["eth"],
  "author": "Денис Верютин <den@pi-art.ru> (https://github.com/yozuul)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.2",
    "fastify": "^3.27.4",
    "fastify-cors": "^6.0.3",
    "node-cron": "^3.0.0",
    "sequelize": "^6.18.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "electron": "^18.0.2",
    "rollup": "^2.70.1"
  }
}
